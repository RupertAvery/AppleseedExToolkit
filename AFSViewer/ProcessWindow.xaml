<Window x:Class="AFSViewer.ProcessWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AFSViewer"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance local:ProcessWindowModel}"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        Title="ProcessWindow" Height="200" Width="400">
    <Grid>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="{Binding Message}"></TextBlock>
    </Grid>
</Window>
